{
  "compilerOptions": { 
    "strictNullChecks": true,
    "forceConsistentCasingInFileNames": true,
    "outDir": "./public/js",
    "sourceMap": true,
    "noImplicitAny": false,
    "module": "esnext", 
    "moduleResolution": "Node",
    "target": "es6",
    "jsx": "react",
    "allowJs": true,
    "lib": ["es2017", "dom"], 
  },
  "include": [
    "./src/**/*"
  ]
}